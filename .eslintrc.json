{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "no-underscore-dangle": 0,
    "no-return-await": 0,
    "no-console": 0,
    "no-unused-vars": "warn",
    "consistent-return": 0,
    "no-restricted-syntax": "warn",
    "no-undef": "warn",
    "no-await-in-loop": "warn"
  }
}
